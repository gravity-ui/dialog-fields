{
  "name": "@gravity-ui/dialog-fields",
  "version": "7.0.0",
  "description": "The library helps to create dialog with forms by using registered field-types with ability to add new ones.",
  "license": "MIT",
  "repository": {
    "type": "arc",
    "url": "https://github.com/gravity-ui/dialog-fields.git"
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "files": [
    "build",
    "assets"
  ],
  "scripts": {
    "lint": "npm run lint:code && npm run lint:styles && npm run lint:other",
    "lint:fix": "npm run lint:code:fix && npm run lint:styles:fix && npm run lint:other:fix",
    "lint:code": "eslint src",
    "lint:code:fix": "eslint src --fix",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint:styles:fix": "stylelint 'src/**/*.scss' --fix",
    "lint:other": "prettier --check '**/*.{md,yaml,yml,json}'",
    "lint:other:fix": "prettier --write '**/*.{md,yaml,yml,json}'",
    "typecheck": "tsc --noEmit",
    "build:lib": "ui-core build",
    "build": "build-storybook -c .storybook -o build",
    "dev": "start-storybook -p 8888 -c .storybook",
    "prepublishOnly": "rm -rf lib && npm run build:lib",
    "storybook:build": "build-storybook -c .storybook -o storybook-static",
    "storybook:extract": "npx sb extract storybook-static",
    "ci:storybook:build": "npm run storybook:build"
  },
  "dependencies": {
    "@gravity-ui/i18n": "^1.0.0",
    "final-form": "^4.20.7",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "react-error-boundary": "^3.1.3",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.3",
    "react-final-form-listeners": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@gravity-ui/eslint-config": "^1.0.2",
    "@gravity-ui/i18n": "^1.0.0",
    "@gravity-ui/prettier-config": "^1.0.1",
    "@gravity-ui/stylelint-config": "^1.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@gravity-ui/uikit": "^3.0.2",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.9",
    "@types/lodash": "^4.14.175",
    "@types/query-string": "^5.1.0",
    "@types/rc-slider": "^8.6.6",
    "@types/react": "^16.14.16",
    "@types/react-dom": "^16.9.14",
    "@types/react-final-form-listeners": "^1.0.0",
    "@yandex-data-ui/ui-core": "^3.3.0",
    "axios": "^0.18.0",
    "bem-cn-lite": "^4.0.0",
    "eslint": "^8.23.1",
    "lodash": "~4.17.13",
    "lodash.isequal": "^4.5.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prop-types": "^15.6.0",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "sass": "^1.56.1",
    "sass-loader": "^10.2.1",
    "style-loader": "^2.0.0",
    "stylelint": "^14.12.0",
    "svg-sprite-loader": "~4.1.6",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "@gravity-ui/i18n": "^1.0.0",
    "@gravity-ui/uikit": "^3.0.0",
    "bem-cn-lite": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "ui-core": {
    "lib": {
      "internalDirs": [
        "stories"
      ]
    }
  }
}
